{
    "name": "market-bot",
    "private": true,
    "author": "Jeroen Akkerman",
    "version": "2.3.0",
    "description": "A Discord bot for looking up EVE Online market prices",
    "repository": {
        "type": "git",
        "url": "https://github.com/Ionaru/MarketBot.git"
    },
    "scripts": {
        "clean": "npx rimraf -q dist",
        "build": "npm run clean && tsc --skipLibCheck --project tsconfig.json",
        "lint": "npm run lint:src && npm run lint:test",
        "lint:src": "eslint --ext ts --max-warnings 0 --ignore-pattern *.spec.ts src",
        "lint:test": "eslint --ext ts --plugin jest --env jest/globals src/**/*.spec.ts",
        "pretest": "npm run lint",
        "test": "jest src",
        "start": "npm run build && node dist/src/index.js",
        "start:ts": "TS_NODE_FILES=true node --require ts-node/register src/index.ts"
    },
    "jest": {
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.ts",
            "!src/**/*.d.ts",
            "!src/**/*.spec.ts",
            "!dist/**/*"
        ],
        "preset": "ts-jest",
        "testEnvironment": "node"
    },
    "dependencies": {
        "@bugsnag/js": "^7.5.4",
        "@ionaru/array-utils": "^5.0.0",
        "@ionaru/configurator": "^3.0.0",
        "@ionaru/esi-service": "^5.0.0",
        "@ionaru/eve-utils": "^8.0.0",
        "@ionaru/format-number": "^3.0.1",
        "agentkeepalive": "^4.1.3",
        "axios": "^0.24.0",
        "countdown": "^2.6.0",
        "d3": "^5.16.0",
        "d3-node": "^2.2.2",
        "debug": "^4.3.1",
        "discord.js": "^13.3.1",
        "dotenv": "^10.0.0",
        "elastic-apm-node": "^3.9.0",
        "escape-string-regexp": "^4.0.0",
        "fuse.js": "^6.4.6",
        "http-status-codes": "^2.1.4",
        "moment": "^2.29.1",
        "puppeteer": "^5.5.0",
        "reflect-metadata": "^0.1.13",
        "sqlite3": "^5.0.0",
        "typeorm": "^0.2.29"
    },
    "devDependencies": {
        "@ionaru/eslint-config": "^5.1.0",
        "@types/countdown": "^0.0.8",
        "@types/d3": "^5.16.3",
        "@types/debug": "^4.1.5",
        "@types/jest": "^27.0.2",
        "@types/node": "^16.11.6",
        "@types/puppeteer": "^5.4.2",
        "@types/sqlite3": "^3.1.6",
        "@typescript-eslint/eslint-plugin": "^5.3.0",
        "codecov": "^3.8.1",
        "eslint": "^7.32.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-jest": "^25.2.2",
        "eslint-plugin-no-null": "^1.0.2",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-sonarjs": "^0.10.0",
        "eslint-plugin-unicorn": "^37.0.1",
        "jest": "^27.3.1",
        "ts-jest": "^27.0.7",
        "ts-node": "^10.4.0",
        "typescript": "^4.1.3"
    },
    "license": "MIT"
}
