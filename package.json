{
  "name": "market-bot",
  "private": true,
  "author": "Jeroen Akkerman",
  "version": "2.2.0",
  "description": "A Discord bot for looking up EVE Online market prices",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ionaru/MarketBot.git"
  },
  "scripts": {
    "build": "tsc --skipLibCheck --project tsconfig.json",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "tslint --project tsconfig.json -c tslint.json --format stylish",
    "lint:test": "tslint -c tslint.json src/**/*.spec.ts --format stylish",
    "pretest": "npm run lint",
    "test": "jest src",
    "posttest": "codecov -f coverage/*.json",
    "start": "npm run build && node dist/index.js",
    "start:ts": "TS_NODE_FILES=true node --require ts-node/register src/index.ts"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/**/*.spec.ts"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.d.ts",
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ]
  },
  "dependencies": {
    "@ionaru/typed-events": "0.0.2-5.0",
    "@sentry/node": "^4.4.2",
    "axios": "^0.18.0",
    "countdown": "^2.6.0",
    "d3": "^5.7.0",
    "d3-node": "^2.1.0",
    "discord.js": "^11.4.2",
    "elastic-apm-node": "^2.0.6",
    "escape-string-regexp": "^1.0.5",
    "fuse.js": "^3.3.0",
    "http-status-codes": "^1.3.0",
    "ini": "^1.3.5",
    "js-yaml": "^3.12.0",
    "moment": "^2.23.0",
    "node-fetch": "^2.3.0",
    "puppeteer": "^1.11.0",
    "reflect-metadata": "^0.1.12",
    "sqlite3": "^4.0.4",
    "typeorm": "^0.2.9",
    "typescript": "^3.2.2",
    "winston-pnp-logger": "^1.0.5"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/countdown": "0.0.6",
    "@types/d3": "^5.5.0",
    "@types/elastic-apm-node": "^1.0.0",
    "@types/escape-string-regexp": "^1.0.0",
    "@types/ini": "^1.3.30",
    "@types/jest": "^24.0.9",
    "@types/js-yaml": "^3.11.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "@types/node-fetch": "^2.1.4",
    "@types/puppeteer": "^1.11.1",
    "@types/sinon": "^7.0.2",
    "@types/sqlite3": "^3.1.3",
    "@types/winston": "^2.4.4",
    "chai": "^4.2.0",
    "codecov": "^3.1.0",
    "jest": "^24.3.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "sinon": "^7.2.2",
    "ts-jest": "^24.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "tslint-sonarts": "^1.9.0"
  },
  "license": "MIT"
}
