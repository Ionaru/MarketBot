{
  "name": "market-bot",
  "private": true,
  "author": "Jeroen Akkerman",
  "version": "2.0.0",
  "description": "A Discord bot for looking up EVE Online market prices",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ionaru/MarketBot.git"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "lint": "tslint --project tsconfig.json -c tslint.json && tslint -c tslint.json src/**/*.spec.ts",
    "start": "npm run build && node dist/index.js",
    "start:ts": "TS_NODE_TYPE_CHECK=true node --require ts-node/register src/index.ts",
    "test": "npm run lint && npm run unit",
    "unit": "nyc mocha --require ts-node/register src/**/*.spec.ts",
    "posttest": "nyc report --reporter=json && codecov -f coverage/*.json"
  },
  "nyc": {
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ]
  },
  "dependencies": {
    "countdown": "^2.6.0",
    "d3": "^5.5.0",
    "d3-node": "^2.0.1",
    "discord.js": "^11.3.2",
    "elastic-apm-node": "^1.8.2",
    "escape-string-regexp": "^1.0.5",
    "fuse.js": "^3.2.1",
    "git-rev-sync": "^1.12.0",
    "ini": "^1.3.5",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.12.0",
    "moment": "^2.22.2",
    "puppeteer": "^1.5.0",
    "reflect-metadata": "^0.1.12",
    "sqlite3": "^4.0.1",
    "typeorm": "^0.2.7",
    "typescript": "^2.9.2",
    "winston-pnp-logger": "^1.0.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/countdown": "0.0.6",
    "@types/d3": "^5.0.0",
    "@types/escape-string-regexp": "0.0.32",
    "@types/ini": "^1.3.29",
    "@types/js-yaml": "^3.11.1",
    "@types/mocha": "^5.2.4",
    "@types/node": "^10.5.2",
    "@types/puppeteer": "^1.5.0",
    "@types/sinon": "^5.0.1",
    "@types/sqlite3": "^3.1.3",
    "@types/winston": "^2.3.9",
    "chai": "^4.1.2",
    "codecov": "^3.0.3",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "sinon": "^6.1.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0"
  },
  "license": "MIT"
}
