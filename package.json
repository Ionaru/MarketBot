{
  "name": "market-bot",
  "private": true,
  "author": "Jeroen Akkerman",
  "version": "1.3.1",
  "description": "A Discord bot for looking up EVE Online market prices",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ionaru/MarketBot.git"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "lint": "tslint --project tsconfig.json -c tslint.json && tslint -c tslint.json src/**/*.spec.ts",
    "start": "npm run build && node dist/index.js",
    "start:ts": "node  --require ts-node/register src/index.ts",
    "test": "npm run lint && npm run unit",
    "unit": "nyc mocha --require ts-node/register src/**/*.spec.ts",
    "posttest": "nyc report --reporter=json && codecov -f coverage/*.json"
  },
  "nyc": {
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ]
  },
  "dependencies": {
    "countdown": "^2.6.0",
    "d3": "^4.12.0",
    "d3-node": "^1.1.2",
    "discord.js": "^11.2.1",
    "escape-string-regexp": "^1.0.5",
    "fuse.js": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.10.0",
    "puppeteer": "^0.13.0",
    "sequelize": "^4.22.11",
    "sqlite3": "^3.1.13",
    "typeorm": "^0.1.6",
    "typescript": "^2.6.2",
    "winston-pnp-logger": "^1.0.4"
  },
  "devDependencies": {
    "@types/chai": "^4.0.6",
    "@types/countdown": "0.0.5",
    "@types/d3": "^4.12.0",
    "@types/escape-string-regexp": "0.0.32",
    "@types/js-yaml": "^3.10.1",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.53",
    "@types/puppeteer": "^0.13.4",
    "@types/sequelize": "^4.0.79",
    "@types/sinon": "^4.0.0",
    "@types/sqlite3": "^3.1.1",
    "@types/winston": "^2.3.7",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "sinon": "^4.1.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0"
  },
  "license": "MIT"
}
