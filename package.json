{
  "name": "MarketBot",
  "private": true,
  "author": "Jeroen Akkerman",
  "version": "1.1.0",
  "description": "A Discord bot for looking up EVE Online market prices",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ionaru/MarketBot.git"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "lint": "tslint --project tsconfig.json --type-check -c tslint.json \"src/**/*.ts\"",
    "start": "npm run build && node dist/index.js",
    "start:ts": "node  --require ts-node/register src/index.ts",
    "test": "mocha --require ts-node/register src/**/*.spec.ts"
  },
  "dependencies": {
    "countdown": "^2.6.0",
    "discord.js": "^11.2.1",
    "escape-string-regexp": "^1.0.5",
    "fuse.js": "^3.1.0",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.10.0",
    "mkdirp": "^0.5.1",
    "request": "^2.81.0",
    "sequelize": "^4.8.4",
    "sqlite3": "^3.1.11",
    "typescript": "^2.5.2",
    "winston-pnp-logger": "^1.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/countdown": "0.0.5",
    "@types/escape-string-regexp": "0.0.31",
    "@types/js-yaml": "^3.9.1",
    "@types/mkdirp": "^0.5.1",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.28",
    "@types/request": "^2.0.3",
    "@types/sequelize": "^4.0.73",
    "@types/sinon": "^2.3.4",
    "@types/sqlite3": "^3.1.1",
    "@types/winston": "^2.3.5",
    "chai": "^4.1.2",
    "mocha": "^3.5.3",
    "sinon": "^3.2.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0"
  },
  "license": "MIT"
}
