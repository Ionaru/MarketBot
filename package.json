{
  "name": "market-bot",
  "private": true,
  "author": "Jeroen Akkerman",
  "version": "2.1.1",
  "description": "A Discord bot for looking up EVE Online market prices",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ionaru/MarketBot.git"
  },
  "scripts": {
    "build": "tsc --skipLibCheck --project tsconfig.json",
    "lint": "tslint --project tsconfig.json -c tslint.json && tslint -c tslint.json src/**/*.spec.ts",
    "start": "npm run build && node dist/index.js",
    "start:ts": "TS_NODE_TYPE_CHECK=true node --require ts-node/register src/index.ts",
    "test": "npm run lint && npm run unit",
    "unit": "nyc mocha --require ts-node/register src/**/*.spec.ts",
    "posttest": "nyc report --reporter=json && codecov -f coverage/*.json"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.d.ts",
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "countdown": "^2.6.0",
    "d3": "^5.7.0",
    "d3-node": "^2.1.0",
    "discord.js": "^11.4.2",
    "elastic-apm-node": "^1.14.3",
    "escape-string-regexp": "^1.0.5",
    "fuse.js": "^3.3.0",
    "http-status-codes": "^1.3.0",
    "ini": "^1.3.5",
    "js-yaml": "^3.12.0",
    "moment": "^2.22.2",
    "node-fetch": "^2.3.0",
    "puppeteer": "^1.10.0",
    "reflect-metadata": "^0.1.12",
    "sqlite3": "^4.0.4",
    "typeorm": "^0.2.9",
    "typescript": "^3.1.6",
    "winston-pnp-logger": "^1.0.5"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/countdown": "0.0.6",
    "@types/d3": "^5.0.1",
    "@types/elastic-apm-node": "^1.0.0",
    "@types/escape-string-regexp": "^1.0.0",
    "@types/ini": "^1.3.29",
    "@types/js-yaml": "^3.11.1",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.7",
    "@types/node-fetch": "^2.1.2",
    "@types/puppeteer": "^1.10.0",
    "@types/sinon": "^5.0.5",
    "@types/sqlite3": "^3.1.3",
    "@types/winston": "^2.4.4",
    "chai": "^4.2.0",
    "codecov": "^3.1.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "sinon": "^7.1.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0"
  },
  "license": "MIT"
}
